{
  "dart.openDevTools": "never",
  "dart.flutterHotReloadOnSave": "always",
  "cSpell.words": [
    "appauth",
    "deeplinks",
    "fluttertoast",
    "kuartzo",
    "navapp",
    "pkce",
    "qrcode",
    "raywenderlich",
    "Razeware"
  ],
  "files.watcherExclude": {
    "**/.dart_tool": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    ".flatpak": true
  }
}